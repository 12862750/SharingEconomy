.top-bar {
  height: 44px;
  position: relative;
}

.top-bar .back {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  padding: 0 24rpx;
  display: flex;
  align-items: center;
}

.top-bar .back image {
  width: 20rpx;
  height: 35rpx;
}

.top-bar .title {
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  font-size: 34rpx;
}